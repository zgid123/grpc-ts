{
  "name": "grpc.ts",
  "description": "gRPC for javascript",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "clear": "pnpm -r exec -- rm -rf",
    "clear-node-modules": "pnpm -r exec -- rm -rf node_modules",
    "update-packages": "pnpm update -i -r --latest",
    "build": "turbo run build --filter=!@examples/*"
  },
  "keywords": [
    "grpc",
    "grpc-ts",
    "grpc-js",
    "grpc-typescript",
    "grpc-javascript",
    "grpc-nodejs",
    "grpc-nestjs",
    "grpc-fastify",
    "grpc-cli"
  ],
  "author": "Alpha",
  "repository": {
    "type": "git",
    "url": "git@github.com:zgid123/grpc-ts.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@config/tsconfig": "workspace:^",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.13.0",
    "turbo": "^1.12.5",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  }
}
