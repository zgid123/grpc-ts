{
  "name": "grpc.ts",
  "description": "gRPC for javascript",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "clear": "pnpm -r exec rimraf node_modules dist .turbo lib && pnpm rimraf node_modules .turbo",
    "update-packages": "pnpm update -i -r --latest",
    "build": "turbo run build --filter=!@examples/*",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "grpc",
    "grpc-ts",
    "grpc-js",
    "grpc-typescript",
    "grpc-javascript",
    "grpc-nodejs",
    "grpc-nestjs",
    "grpc-fastify",
    "grpc-cli"
  ],
  "author": "Alpha",
  "repository": {
    "type": "git",
    "url": "git@github.com:zgid123/grpc-ts.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.12.2",
  "devDependencies": {
    "@config/tsconfig": "workspace:^",
    "@eslint/js": "^9.19.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.32.1",
    "turbo": "^2.3.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
